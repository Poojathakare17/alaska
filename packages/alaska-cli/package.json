{
  "name": "alaska-cli",
  "version": "0.11.0",
  "description": "Alaska command line interface",
  "keywords": [
    "alaska"
  ],
  "scripts": {
    "build": "babel ./src --out-dir ./lib",
    "watch": "babel ./src --out-dir ./lib -w",
    "prebuild": "rimraf lib",
    "prepublish": "npm run build"
  },
  "engines": {
    "node": ">=5.0.0"
  },
  "bin": {
    "alaska": "bin/alaska"
  },
  "repository": "https://github.com/maichong/alaska/tree/master/packages/alaska-cli",
  "author": "Liang <liang@maichong.it> (https://github.com/liangxingchen)",
  "license": "MIT",
  "dependencies": {
    "chalk": "^1.1.3",
    "co": "^4.6.0",
    "commander": "^2.9.0",
    "escodegen": "^1.8.1",
    "esprima": "^3.1.3",
    "mkdirp": "^0.5.1",
    "read-promise": "^1.0.0",
    "string-random": "^0.1.0",
    "update-notifier": "^1.0.3"
  },
  "devDependencies": {
    "babel-cli": "^6.22.1",
    "babel-plugin-syntax-flow": "^6.18.0",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-plugin-transform-async-to-generator": "^6.22.0",
    "babel-plugin-transform-class-properties": "^6.22.0",
    "babel-plugin-transform-export-extensions": "^6.22.0",
    "babel-preset-es2015": "^6.22.0",
    "rimraf": "^2.5.4"
  }
}
